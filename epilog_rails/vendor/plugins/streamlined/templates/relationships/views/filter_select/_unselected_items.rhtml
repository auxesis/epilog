<% for item in @all_items %>
    <% unless @items.include?(item) %>
	  <span id="rel_<%= @relationship_name %>_<%= @root.id %>_<%= item.class.name %>_<%= item.id %>"><input type="checkbox" name="item[<%= item.id %>]" id="item_[<%= item.id %>]">&nbsp;<%= item.streamlined_name(@model_ui.relationships[@relationship_name.to_sym].view_def.fields,@model_ui.relationships[@relationship_name.to_sym].view_def.separator) %><br/></span>
	<% end %>
<% end %>
